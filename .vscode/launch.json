{
    "version": "0.2.0",
    "configurations": [
        {
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/src/NodeService/bin/Debug/net5.0/NodeService.dll",
            "args": [],
            "cwd": "${workspaceFolder}/src/NodeService",
            "console": "integratedTerminal",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}